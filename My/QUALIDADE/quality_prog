        Range("AG268") = "X"
        Range("AM268") = ""
    End If
    
    If CheckBox56 = True Then
        Range("G268") = ""
        Range("M268") = ""
        Range("S268") = ""
        Range("AA268") = ""
        Range("AG268") = ""
        Range("AM268") = "X"
    End If

    '---------------------------------
    If CheckBox28 = True Then
        Range("AF274") = "X"
        Range("AN274") = ""
    End If
    If CheckBox29 = True Then
        Range("AF274") = ""
        Range("AN274") = "X"
    End If

    '---------------------------------
    If CheckBox30 = True Then
        Range("O283") = "X"
        Range("V283") = ""
        Range("AE283") = ""
    End If
    
    If CheckBox31 = True Then
        Range("O283") = ""
