    Frame1.Visible = True
    Frame2.Visible = False
    
    ComboBox1 = ""
    TextBox1 = ""
    TextBox2 = ""
    TextBox3 = ""
    TextBox4 = ""
    TextBox5 = ""
    
    
End Sub










