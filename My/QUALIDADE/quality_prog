        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .ThemeFont = xlThemeFontNone
    End With
    
    '------------------------------------------------------------- E: Type of test
