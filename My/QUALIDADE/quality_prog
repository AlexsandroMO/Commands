Private Sub UserForm_Initialize()

Dim lista_user As Variant


Frame2.Visible = False
Frame1.Visible = True

    lista_user = Array("")
    
    '---USER------------------------------------------------------------
    Windows("PREENCHE_TEMPLATES_QUALIDADE.xlsm").Activate
    Sheets("INSPECTOR").Select

    cont_user = Sheets("INSPECTOR").Range("A" & Rows.Count).End(xlUp).Row
    
    cont = 0
    For i = 1 To cont_user
        ReDim Preserve lista_user(cont)
        lista_user(UBound(lista_user)) = Range("A" & i & "").Value
        cont = cont + 1
        
    Next i
    
    
    For j = 1 To UBound(lista_user)
        ComboBox1.AddItem lista_user(j)
    Next j
    
    
End Sub

Private Sub CommandButton1_Click()

Dim lista_user As Variant
Dim lista_insp As Variant
Dim lista_code As Variant
Dim lista_sign As Variant
Dim lista_marinha1 As Variant
Dim lista_marinha2 As Variant
Dim lista_stamp1 As Variant
Dim lista_stamp2 As Variant
Dim lista_pass As Variant
Dim lista_id As Variant

    lista_user = Array("")
    lista_insp = Array("")
    lista_code = Array("")
    lista_sign = Array("")
    lista_marinha1 = Array("")
    lista_marinha2 = Array("")
    lista_stamp1 = Array("")
    lista_stamp2 = Array("")
    lista_pass = Array("")
    lista_id = Array("")
    
    name_inspector = ComboBox1
    pass = TextBox1

    '---USER------------------------------------------------------------
    Windows("PREENCHE_TEMPLATES_QUALIDADE.xlsm").Activate
    Sheets("INSPECTOR").Select

    cont_user = Sheets("INSPECTOR").Range("A" & Rows.Count).End(xlUp).Row
    
    cont = 0

    For i = 1 To cont_user
        ReDim Preserve lista_user(cont)
        ReDim Preserve lista_insp(cont)
        ReDim Preserve lista_code(cont)
        ReDim Preserve lista_sign(cont)
        ReDim Preserve lista_marinha1(cont)
        ReDim Preserve lista_marinha2(cont)
        ReDim Preserve lista_stamp1(cont)
        ReDim Preserve lista_stamp2(cont)
        ReDim Preserve lista_pass(cont)
        ReDim Preserve lista_id(cont)
        
        lista_user(UBound(lista_user)) = Range("A" & i & "").Value
        lista_insp(UBound(lista_insp)) = Range("B" & i & "").Value
        lista_code(UBound(lista_code)) = Range("C" & i & "").Value
        lista_sign(UBound(lista_sign)) = Range("D" & i & "").Value
        lista_marinha1(UBound(lista_marinha1)) = Range("E" & i & "").Value
        lista_marinha2(UBound(lista_marinha2)) = Range("F" & i & "").Value
        lista_stamp1(UBound(lista_stamp1)) = Range("G" & i & "").Value
        lista_stamp2(UBound(lista_stamp2)) = Range("H" & i & "").Value
        lista_pass(UBound(lista_pass)) = Range("BC" & i & "").Value
        lista_id(UBound(lista_id)) = Range("I" & i & "").Value
        cont = cont + 1
    Next i
    
    For j = 1 To UBound(lista_user)
        If lista_user(j) = name_inspector Then
        
            If lista_pass(j) = pass Then
                Windows("PREENCHE_TEMPLATES_QUALIDADE.xlsm").Activate
                Sheets("Reserve").Select
                
                Columns("A:A").Select
                Selection.ClearContents
   
                Range("A1") = lista_user(j)
                Range("A2") = lista_insp(j)
                Range("A3") = lista_code(j)
                Range("A4") = lista_sign(j)
                Range("A5") = lista_marinha1(j)
                Range("A6") = lista_marinha2(j)
                Range("A7") = lista_stamp1(j)
                Range("A8") = lista_stamp2(j)
               
                UserForm2.Hide
                UserForm1.Show
                
            Else
                MsgBox "Senha incorreta. Favor digite sua senha!"
                TextBox1 = ""

            End If
            
        End If
        
    Next j
    
 
End Sub

Private Sub CommandButton2_Click()
    
    TextBox2 = ComboBox1
    TextBox3 = TextBox1
    
    Frame1.Visible = False
    Frame2.Visible = True
    

End Sub

Private Sub CommandButton3_Click()

Dim lista_user, lista_pass, lista_id As Variant

    lista_user = Array("")
    lista_pass = Array("")
    lista_id = Array("")

    '---USER------------------------------------------------------------
    Windows("PREENCHE_TEMPLATES_QUALIDADE.xlsm").Activate
    Sheets("INSPECTOR").Select

    cont_user = Sheets("INSPECTOR").Range("A" & Rows.Count).End(xlUp).Row
    
    cont = 0
    For i = 1 To cont_user
        ReDim Preserve lista_user(cont)
        ReDim Preserve lista_pass(cont)
        ReDim Preserve lista_id(cont)
        lista_user(UBound(lista_user)) = Range("A" & i & "").Value
        lista_pass(UBound(lista_pass)) = Range("BC" & i & "").Value
        lista_id(UBound(lista_id)) = Range("I" & i & "").Value
        cont = cont + 1
        
    Next i

    For j = 1 To UBound(lista_user)

        If TextBox2 = lista_user(j) Then
        
            If TextBox3 = lista_pass(j) Then
                If TextBox4 = TextBox5 Then
                    Columns("I:I").Select
                    
                    Selection.Find(What:=lista_id(j), After:=ActiveCell, LookIn:=xlFormulas, LookAt _
                        :=xlPart, SearchOrder:=xlByRows, SearchDirection:=xlNext, MatchCase:= _
                        False, SearchFormat:=False).Activate

                    lin = ActiveCell.Row
                    col = ActiveCell.Column
                    
                    Range(Cells(lin, col + 46), Cells(lin, col + 46)) = TextBox4
    
                    ComboBox1 = ""
                    TextBox1 = ""
                    TextBox2 = ""
                    TextBox3 = ""
                    TextBox4 = ""
                    TextBox5 = ""
                    Frame1.Visible = True
                    Frame2.Visible = False

                Else
                
                    MsgBox "A duas senhas digitadas precisam ser iguais!"
                    TextBox4 = ""
                    TextBox5 = ""
                
                End If
                
                
            Else
                MsgBox "A senha do usuário não confere com a registrada no banco. Procure o administrador do sistema!"
                ComboBox1 = ""
                TextBox1 = ""
                TextBox2 = ""
                TextBox3 = ""
                TextBox4 = ""
                TextBox5 = ""
                Frame1.Visible = True
                Frame2.Visible = False
            End If
             
        
        
        End If

    Next j
    
    If TextBox2 <> "" And TextBox3 <> "" And TextBox4 <> "" And TextBox5 <> "" Then
        If TextBox4 = TextBox5 Then
            lin = UBound(lista_user) + 2
            Range("A" & lin & "").Value = TextBox2
            Range("BC" & lin & "").Value = TextBox4
            Range("I" & lin & "").Value = UBound(lista_user) + 1
            ComboBox1 = ""
            TextBox1 = ""
            TextBox2 = ""
            TextBox3 = ""
            TextBox4 = ""
            TextBox5 = ""
            Frame1.Visible = True
            Frame2.Visible = False
            MsgBox "Usuário cadastrado com Sucesso. [Atenção], só após a validadação de dados será possível assinar documentos digitalmente!"

        Else
                MsgBox "A duas senhas digitadas precisam ser iguais!"
                TextBox4 = ""
                TextBox5 = ""
        End If
    End If


End Sub

